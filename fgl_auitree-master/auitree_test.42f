          <Button name="current_folderpage_test" width="14" text="Current Test Folder Page" posY="6" posX="1" gridWidth="14" tabIndex="31"/>
          <Label text=" " posY="7" posX="1" gridWidth="3"/>
          <Label text="This method will look at the field with focus and tell you what folder page the" posY="8" posX="0" gridWidth="79"/>
          <Label text="field with focus is on" posY="9" posX="0" gridWidth="22"/>
          <Button name="current_folderpage_toplevel" width="14" text="Current TopLevel Folder Page" posY="10" posX="1" gridWidth="14" tabIndex="32"/>
        </Grid>
