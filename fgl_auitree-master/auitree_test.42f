<Form name="auitree_test" build="5.00.01" text="AUI Tree Menu example" width="123" height="18">
  <ActionDefaultList>
    <ActionDefault name="copyentirearraywithheadings" text="Copy Entire Array  (incl Headings)" comment="Copy the entire array including table columnheadings to the clipboard" acceleratorName="control-e"/>
    <ActionDefault name="copyentirearraywithoutheadings" text="Copy Entire Array (excl Headings)" comment="Copy the entire array excluding table column headings to the clipboard" acceleratorName="control-e"/>
