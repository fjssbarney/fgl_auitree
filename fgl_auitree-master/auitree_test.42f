  <Folder name="auitree_test">
    <Page name="home" text="Welcome" action="home">
      <Grid width="79" height="18">
        <Label text="This program gives some examples of where AUI (or DOM) tree manipulation has" posY="0" posX="0" gridWidth="76"/>
        <Label text="been used to achieve a particular outcome." posY="1" posX="0" gridWidth="42"/>
        <Label text=" " posY="2" posX="1" gridWidth="3"/>
        <Label text="Before attempting to learn aspects of this manipulation, you should start the" posY="3" posX="0" gridWidth="77"/>
        <Label text="GDC with the -D command line option specified and be familiar with what happens" posY="4" posX="0" gridWidth="79"/>
        <Label text="if you press Control and right-click on a widget. Any manipulation we are doing" posY="5" posX="0" gridWidth="79"/>
        <Label text="is essentially a case of finding one or more of the nodes in the tree and" posY="6" posX="0" gridWidth="73"/>
        <Label text="reading and changing one of the attribute values." posY="7" posX="0" gridWidth="49"/>
        <Label text=" " posY="8" posX="1" gridWidth="3"/>
        <Label text="In creating this example I came across a number of instance where 4Js has" posY="9" posX="0" gridWidth="73"/>
        <Label text="implemented a function that I had forgotten existed. So with every release" posY="10" posX="0" gridWidth="74"/>
        <Label text="keep an eye on ui.form and ui.Dialog classes to see what methods are there" posY="11" posX="0" gridWidth="74"/>
        <Label text="for you to use." posY="12" posX="0" gridWidth="15"/>
        <Label text=" " posY="13" posX="1" gridWidth="3"/>
        <Label text="DISCLAIMER" posY="14" posX="0" gridWidth="10"/>
        <Label text="4Js may change the AUI tree between Genero releases. Ensure that you test any" posY="15" posX="0" gridWidth="77"/>
        <Label text="library routines you create with each new release." posY="16" posX="0" gridWidth="50"/>
        <FormField name="formonly.dummy" colName="dummy" fieldId="20" sqlTabName="formonly" tabIndex="1">
          <Edit width="1" isPassword="1" style="dummy" posY="17" posX="1" gridWidth="1"/>
        </FormField>
      </Grid>
    </Page>
    <Page name="readattributes" text="Read Attributes">
      <VBox>
        <VBox name="vbx1">
          <HBox name="hbx1">
            <Group name="grp11">
              <Grid name="grd11" width="15" height="1">
                <Label text="Top left corner" posY="0" posX="0" gridWidth="15"/>
              </Grid>
            </Group>
            <Group name="grp12">
              <Grid name="grd12" width="16" height="1">
                <Label text="Top right corner" posY="0" posX="0" gridWidth="16"/>
              </Grid>
            </Group>
          </HBox>
          <HBox name="hbx2">
            <Group name="grp21">
              <Grid name="grd21" width="18" height="1">
                <Label text="Bottom left corner" posY="0" posX="0" gridWidth="18"/>
              </Grid>
            </Group>
            <Group name="grp22">
              <Grid name="grd22" width="19" height="1">
                <Label text="Bottom right corner" posY="0" posX="0" gridWidth="19"/>
              </Grid>
            </Group>
          </HBox>
        </VBox>
        <Grid width="75" height="5">
          <Label text="Whilst methods exist to set attributes, the methods to get them may not be" posY="0" posX="0" gridWidth="74"/>
          <Label text="present. This example uses the function field_hidden_get() to determine the" posY="1" posX="0" gridWidth="75"/>
          <Label text="current hidden attribute before using the official method to either" posY="2" posX="0" gridWidth="67"/>
          <Label text="hide or unhide the element." posY="3" posX="0" gridWidth="27"/>
          <FormField name="formonly.dummy2" colName="dummy2" fieldId="21" sqlTabName="formonly" tabIndex="2">
            <Edit width="1" isPassword="1" style="dummy" posY="4" posX="1" gridWidth="1"/>
          </FormField>
        </Grid>
      </VBox>
    </Page>
    <Page name="readdialogtype" text="Read Dialog Type">
      <Grid width="123" height="7">
        <Label text="Construct" posY="0" posX="0" gridWidth="9"/>
        <FormField name="formonly.constructfield" colName="constructfield" fieldId="4" sqlTabName="formonly" tabIndex="3">
          <ButtonEdit width="9" action="dialogtype" posY="0" posX="15" gridWidth="11"/>
        </FormField>
        <Label text="Click the ButtonEdit, should display Construct" posY="0" posX="28" gridWidth="46"/>
        <Label text="Input" posY="1" posX="0" gridWidth="5"/>
        <FormField name="formonly.inputfield" colName="inputfield" fieldId="5" sqlTabName="formonly" tabIndex="4">
          <ButtonEdit width="9" action="dialogtype" posY="1" posX="15" gridWidth="11"/>
        </FormField>
        <Label text="Click the ButtonEdit, should display Input" posY="1" posX="28" gridWidth="42"/>
        <Label text="Menu" posY="2" posX="0" gridWidth="4"/>
        <Button name="menu_test" width="11" posY="2" posX="15" gridWidth="11" tabIndex="5"/>
        <Label text="Click the Button, then click Test Button, should display Menu" posY="2" posX="28" gridWidth="61"/>
        <Label text="Display Array" posY="3" posX="0" gridWidth="14"/>
        <FormField name="formonly.display_field" colName="display_field" fieldId="6" sqlTabName="formonly" tabIndex="6">
          <Edit width="11" posY="3" posX="15" gridWidth="11"/>
        </FormField>
        <Label text="Select the Display Array, then click Test in Action Panel, should display DisplayArray" posY="3" posX="28" gridWidth="86"/>
        <Label text="Input Array" posY="4" posX="0" gridWidth="11"/>
        <FormField name="formonly.input_field" colName="input_field" fieldId="7" sqlTabName="formonly" tabIndex="7">
          <Edit width="11" posY="4" posX="15" gridWidth="11"/>
        </FormField>
        <Label text="Select the Input Array, then click Test in Action Panel, should display InputArray" posY="4" posX="28" gridWidth="82"/>
        <Label text=" " posY="5" posX="1" gridWidth="3"/>
        <Label text="Usage for this maybe in a zoom window where you may allow multiple selection in a CONSTRUCT which you wouldn&apos;t in an INPUT." posY="6" posX="0" gridWidth="123"/>
      </Grid>
    </Page>
    <Page name="hidetag" text="Hide TAG">
      <Grid width="79" height="15">
        <Label width="15" text="Field 01" tag="tfield01" posY="0" posX="1" gridWidth="15"/>
        <FormField name="formonly.tcode01" colName="tcode01" fieldId="0" sqlTabName="formonly" tabIndex="8">
          <ButtonEdit width="8" tag="tfield01" posY="0" posX="17" gridWidth="10"/>
        </FormField>
        <FormField name="formonly.tdesc01" colName="tdesc01" fieldId="1" sqlTabName="formonly" noEntry="1" tabIndex="9">
          <Label width="23" tag="tfield01" posY="0" posX="28" gridWidth="23"/>
        </FormField>
        <Label width="15" text="Field 02" tag="tfield02" posY="1" posX="1" gridWidth="15"/>
        <FormField name="formonly.tcode02" colName="tcode02" fieldId="2" sqlTabName="formonly" tabIndex="10">
          <ButtonEdit width="8" tag="tfield02" posY="1" posX="17" gridWidth="10"/>
        </FormField>
        <FormField name="formonly.tdesc02" colName="tdesc02" fieldId="3" sqlTabName="formonly" noEntry="1" tabIndex="11">
          <Label width="23" tag="tfield02" posY="1" posX="28" gridWidth="23"/>
        </FormField>
        <Label text=" " posY="2" posX="1" gridWidth="3"/>
        <Label text="Sometimes you may want to work on more than one form element at a time. In this" posY="3" posX="0" gridWidth="79"/>
        <Label text="example, have used the TAG attribute to select more than one field to" posY="4" posX="0" gridWidth="69"/>
        <Label text="perform the DOM manipulation on." posY="5" posX="0" gridWidth="32"/>
        <Label text="Personally I dislike using TAG as you can only have one dimension of data in it" posY="7" posX="0" gridWidth="79"/>
        <Label text="and still use XPath. If we had matches in the XPath method it would be more" posY="8" posX="0" gridWidth="75"/>
        <Label text="useful." posY="9" posX="0" gridWidth="7"/>
        <Label text="Also note this routine hides the field manipulating the DOM directly rather" posY="11" posX="0" gridWidth="75"/>
        <Label text="than using the Genero method. If possibleyou should use the Genero method" posY="12" posX="0" gridWidth="73"/>
        <Label text="where available." posY="13" posX="0" gridWidth="16"/>
        <FormField name="formonly.dummy3" colName="dummy3" fieldId="22" sqlTabName="formonly" tabIndex="12">
          <Edit width="1" isPassword="1" style="dummy" posY="14" posX="1" gridWidth="1"/>
        </FormField>
      </Grid>
    </Page>
    <Page name="arraycopy" text="Array Copy">
      <Table pageSize="8" tabName="arrcopy">
        <TableColumn name="formonly.accode" colName="accode" fieldId="8" sqlTabName="formonly" text="Code" tabIndex="13">
          <Edit width="11"/>
        </TableColumn>
        <TableColumn name="formonly.acdesc" colName="acdesc" fieldId="9" sqlTabName="formonly" text="Description" tabIndex="14">
          <Edit width="20"/>
        </TableColumn>
        <TableColumn name="formonly.acactual" colName="acactual" fieldId="10" sqlTabName="formonly" text="Actual" tabIndex="15">
          <Edit width="13" format="$,$$$,$$&amp;.&amp;&amp;"/>
        </TableColumn>
        <TableColumn name="formonly.acbudget" colName="acbudget" fieldId="11" sqlTabName="formonly" text="Budget" tabIndex="16">
          <Edit width="13" format="$,$$$,$$&amp;.&amp;&amp;"/>
        </TableColumn>
        <TableColumn name="formonly.acvariance" colName="acvariance" fieldId="12" sqlTabName="formonly" text="Variance" tabIndex="17">
          <Edit width="14" format="----,-$&amp;.&amp;&amp;"/>
        </TableColumn>
        <TableColumn name="formonly.aclastuseddate" colName="aclastuseddate" fieldId="13" sqlTabName="formonly" text="Last Used Date" tabIndex="18">
          <DateEdit width="9" format="dd-mmm-yyyy"/>
        </TableColumn>
        <TableColumn name="formonly.aclastusedtime" colName="aclastusedtime" fieldId="14" sqlTabName="formonly" text="Last Used Time" tabIndex="19">
          <TimeEdit width="9"/>
        </TableColumn>
        <TableColumn name="formonly.aclastuseddatetime" colName="aclastuseddatetime" fieldId="15" sqlTabName="formonly" text="Last Used Datetime" tabIndex="20">
          <Edit width="21" scroll="1"/>
        </TableColumn>
      </Table>
    </Page>
    <Page name="styleaddremove" text="Stye Add/Remove">
      <Grid width="105" height="8">
        <Label text="Field 1" posY="0" posX="0" gridWidth="8"/>
        <FormField name="formonly.sfield01" colName="sfield01" fieldId="16" sqlTabName="formonly" notNull="1" required="1" tabIndex="21">
          <Edit width="8" style="mandatory" posY="0" posX="9" gridWidth="8"/>
        </FormField>
        <Label text="This field is mandatory" posY="0" posX="19" gridWidth="23"/>
        <Label text="Field 2" posY="1" posX="0" gridWidth="8"/>
        <FormField name="formonly.sfield02" colName="sfield02" fieldId="17" sqlTabName="formonly" notNull="1" required="1" tabIndex="22">
          <Edit width="8" style="mandatory" posY="1" posX="9" gridWidth="8"/>
        </FormField>
        <Label text="This field is mandatory and the number must be even and greater than zero" posY="1" posX="19" gridWidth="73"/>
        <Label text="Field 3" posY="2" posX="0" gridWidth="8"/>
        <FormField name="formonly.sfield03" colName="sfield03" fieldId="18" sqlTabName="formonly" tabIndex="23">
          <Edit width="8" posY="2" posX="9" gridWidth="8"/>
        </FormField>
        <Label text="This field is not mandatory but if it is entered it must be even and greater than zero" posY="2" posX="19" gridWidth="86"/>
        <Label text="In this example, we add and remove a style to a field with an existing style." posY="4" posX="0" gridWidth="77"/>
        <Label text="Mandatory fields have a pink background." posY="5" posX="0" gridWidth="40"/>
        <Label text="Invalid values have a red background." posY="6" posX="0" gridWidth="37"/>
        <Label text="Note what happens when you enter 1212 slowly into each field." posY="7" posX="0" gridWidth="61"/>
      </Grid>
    </Page>
