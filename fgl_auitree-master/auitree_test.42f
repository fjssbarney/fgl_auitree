    <Page name="currentfolderpage" text="Current Folder Page">
      <VBox>
        <Grid width="79" height="11">
          <Label text="This example shows the conundrum for folder pages and determining what page is" posY="0" posX="0" gridWidth="78"/>
          <Label text="on top." posY="1" posX="0" gridWidth="7"/>
          <Label text="The conundrum exists because the Folder node in the AUI Tree does not" posY="1" posX="9" gridWidth="69"/>
